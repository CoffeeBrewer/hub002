{
  "name": "workspace-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node dev.js",
    "dev:server": "npm --prefix server run dev",
    "dev:client": "npm --prefix client run dev",
